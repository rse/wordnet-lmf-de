{
    "name":        "wordnet-lmf-de",
    "version":     "0.9.0",
    "description": "WordNet Lexical Markup Framework (LMF): German (DE)",
    "keywords":    [ "wordnet", "lmf", "de", "german" ],
    "main":        "wordnet-lmf-de.js",
    "repository": {
        "type": "git",
        "url":  "https://github.com/rse/wordnet-lmf-de.git"
    },
    "author": {
        "name":  "Ralf S. Engelschall",
        "email": "rse@engelschall.com",
        "url":   "http://engelschall.com"
    },
    "license":  "CC-BY-SA-4.0",
    "homepage": "https://github.com/rse/wordnet-lmf-de",
    "bugs":     "https://github.com/rse/wordnet-lmf-de/issues",
    "devDependencies": {
    },
    "dependencies": {
        "npm-run-all":             "~4.1.3",
        "npm-install-fetch":       "0.9.3",
        "wordnet-lmf":             "0.9.2"
    },
    "peerDependencies": {
        "wordnet-lmf":             "0.9.2"
    },
    "engines": {
        "node":                    ">=8.0.0"
    },
    "scripts": {
        "install":  "npm-run-all -s fetch import",
        "fetch":    "npm-install-fetch",
        "import":   "wordnet-lmf -d wordnet-lmf-de.db import -p easysax -e 2 wordnet-lmf-de.xml"
    },
    "npm-install-fetch": [ {
        "name":     "Open DE WordNet 1.0 (2018-05-11) [26 MB, CC-BY-SA-4.0]",
        "input":    "https://github.com/hdaSprachtechnologie/odenet/raw/98064d221070b1de7cfbfaa4ca48a2ab6c504bec/deWordNet.xml",
        "output":   "wordnet-lmf-de.xml"
    } ]
}
